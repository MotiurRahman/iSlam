<%- contentFor('body')%>  



<div class="container">
  <br>
  <!-- Page Heading/Breadcrumbs -->
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">Insert
        <small>Subheading</small>
      </h1>
      <div class="col-md-6 col-md-offset-3" style="text-align: center;">
